<div class="flexed">
  <% @schools.each do |n| %>
  <div class="panel <%= n.name %>-1">
    <div class="panel-body">
    <%= link_to n.name, school_path(n) %>
    </div>
  </div>
  <% end %>
  <div class="panel more-coming">
    <div class="panel-body">
      <%= link_to "More schools coming", "#" %>
    </div>
  </div>
</div>
